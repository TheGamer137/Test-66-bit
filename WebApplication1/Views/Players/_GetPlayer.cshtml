@model IEnumerable<Player>

@foreach (Player p in Model)
{
    <tr id="@p.Id" class="text-center">
        <td class="1">@p.Name</td>
        <td class="2">@p.Surname</td>
        <td class="3">@p.Gender</td>
        <td class="4">@p.Birth</td>
        <td class="5">@p.Team?.Name</td>
        <td class="6">@p.Country</td>
        <td>
            <a asp-action="Upsert" asp-route-id="@p.Id" asp-controller="Players" class="btn btn-outline-dark form-control">
                Редактировать
            </a>
        </td>
    </tr>
}